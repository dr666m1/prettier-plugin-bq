{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier" // NOTE make sure to put it last
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    },
    "plugins": [
        "@typescript-eslint",
        "unicorn"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": ["warn", {"varsIgnorePattern": "^_$"}],
        "unicorn/no-unused-properties": "warn"
    }
}
